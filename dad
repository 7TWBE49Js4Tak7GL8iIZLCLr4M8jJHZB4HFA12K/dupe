wait(4)
if game:IsLoaded() then 
wait()
local playerName = game:GetService("Players").LocalPlayer.Name;
local whitelistedNames = {
["DuelsLegend"] = true;
["Qoucx"] = true;
["MistakenStudios"] = true;
["Cool_Doge68"] = true;
["Leon_Jka"] = true;
["thechosenizaiah"] = true;
